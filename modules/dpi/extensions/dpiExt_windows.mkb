#!/usr/bin/env mkb
# Builder mkb file for the dpiExt extension on windows

platform WIN32

options
{
    dll
    x86
    output-name="lib/win32/dpiExt.dll"
    output-name-debug="lib/win32/dpiExt_d.dll"
}
define S3E_BUILD_EXT

subproject dpiExt_build
library
    {
        user32.lib
	gdi32.lib
    }
