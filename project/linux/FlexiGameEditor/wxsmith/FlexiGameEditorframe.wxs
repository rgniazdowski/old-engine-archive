<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="FlexiGameEditorFrame">
		<title>FlexiGame::Editor 1.0</title>
		<size>640,480</size>
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer2" member="no">
					<orient>wxVERTICAL</orient>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>5</option>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>En&amp;gine</label>
				<object class="wxMenuItem" name="idMenuEngineInitialize" variable="MenuItem3" member="yes">
					<label>Initialize</label>
					<accel>Alt-R</accel>
					<help>Initialize the internal FlexiGame::Engine</help>
					<handler function="OnMenuEngineInitialize" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuEngineQuit" variable="MenuItem11" member="yes">
					<label>Quit</label>
					<accel>Alt+Q</accel>
					<help>Close the game engine - exit the game</help>
					<handler function="OnMenuEngineQuit" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="idMenuEngineFreezeFrame" variable="MenuItem10" member="yes">
					<label>Freeze frame</label>
					<accel>Alt-P</accel>
					<help>Pause/freeze current frame</help>
					<handler function="OnMenuEngineFreezeFrame" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu4" member="yes">
				<label>&amp;Tools</label>
				<object class="wxMenuItem" name="idMenuToolsOptions" variable="MenuItem4" member="yes">
					<label>Options</label>
					<help>Edit options for FlexiGame::Editor</help>
				</object>
			</object>
			<object class="wxMenu" variable="Menu5" member="yes">
				<label>G&amp;it</label>
				<object class="wxMenuItem" name="idMenuGitRunGitGUI" variable="MenuItem5" member="yes">
					<label>Run Git GUI</label>
					<help>Execute the git gui application in project root folder</help>
				</object>
				<object class="wxMenuItem" name="idMenuGitRunGitk" variable="MenuItem6" member="yes">
					<label>Run Gitk</label>
					<help>Run the gitk tool for visualing branches and commits</help>
				</object>
				<object class="wxMenuItem" name="idMenuGitShowLog" variable="MenuItem7" member="yes">
					<label>Show log</label>
					<help>Show the git commits log</help>
				</object>
				<object class="wxMenuItem" name="idMenuGitOpenGitconfig" variable="MenuItem8" member="yes">
					<label>Open .gitconfig</label>
					<help>Open for editing the main git configuration file for this project</help>
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuHelpLocalDocs" variable="MenuItem9" member="yes">
					<label>Local docs</label>
					<help>Open HTML viewer with documentation (local)</help>
				</object>
				<object class="wxMenuItem" name="idMenuHelpAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
