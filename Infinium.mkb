defines
{
	FG_NO_UNDEF
	TIXML_USE_STL
	FG_USING_LUA_PLUS
}

options
{
	module_path="modules"
	arm-arch=armv6
	arm-cpu=6
	arm-fpu=vfp
	optimise-speed
	#enable-exceptions
	warn-level=2
	buildenv=VC11
    cflags="-std=c++11"
	s3e-data-dir=data

}

packages
{
	libjpeg
	libpng
	#zlib
}

subprojects
{
	iwutil
	dpi
	tinyobj
	tinyxml
	stbfont
	ng_luaplus
	zlib_128
	simpleopt
}

includepaths
{
	src
	modules/glm
    modules
}

librarypaths
{
	modules/ng_luaplus/lib
	modules/zlib_128/lib
	modules/libjpeg/lib
}

