defines
{
	FG_NO_UNDEF
	TIXML_USE_STL
	FG_USING_LUA_PLUS
}

options
{
	module_path="modules"
	arm-arch=armv6
	arm-cpu=6
	arm-fpu=vfp
	optimise-speed
	enable-exceptions
	warn-level=2
	buildenv=VC11
    cflags="-std=c++11"
}

packages
{
	libjpeg
	libpng
	zlib
}

subprojects
{
	iwutil
	dpi
	tinyobj
	tinyxml
	stbfont
	ng_luaplus
}

includepaths
{
	src
	modules/glm
}

librarypaths
{
	modules/ng_luaplus/lib
}

